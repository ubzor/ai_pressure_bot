datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["driverAdapters"]
    output          = "./generated/client"
}

model Entry {
    id             Int      @id @default(autoincrement())
    telegramUserId String
    systolic       Int
    diastolic      Int
    pulse          Int
    createdAt      DateTime @default(now())
}
