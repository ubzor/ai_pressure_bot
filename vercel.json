{
    "builds": [
        { "src": "api/**/*.ts", "use": "@vercel/node" },
        { "src": "src/**/*.ts", "use": "@vercel/node" }
    ],
    "routes": [{ "src": "/api/webhook", "dest": "/api/webhook.ts" }],
    "functions": {
        "api/webhook.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}
